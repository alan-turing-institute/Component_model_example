package component:test-component;

interface resource-interface{
    resource resource-counter{
        constructor();
        do-something: func() -> string;
        add-one: func();
        get-value: func() -> u32;
    }
}

interface logger{
    my-print-log: func(text:string);
}

/// An example world for the component to target.
world example {
    import resource-interface;
    import logger;

    record structure{
        number: u64,
        some-str: string
    }

    record something-else{
        s: string
    }

    resource resource-get-structure{
        constructor();
        get-structure: func(key:string)->structure;
    }

    export get-structure: func()-> structure;
    export resource-interface;
    export logger;
}
